{
    "name": "mechanicsdsl",
    "displayName": "MechanicsDSL",
    "description": "Syntax highlighting, snippets, and language support for MechanicsDSL physics domain-specific language",
    "version": "1.5.0",
    "publisher": "mechanicsdsl",
    "repository": {
        "type": "git",
        "url": "https://github.com/MechanicsDSL/mechanicsdsl"
    },
    "icon": "images/icon.png",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "keywords": [
        "physics",
        "mechanics",
        "lagrangian",
        "hamiltonian",
        "simulation",
        "dsl"
    ],
    "contributes": {
        "languages": [
            {
                "id": "mechanicsdsl",
                "aliases": [
                    "MechanicsDSL",
                    "mdsl"
                ],
                "extensions": [
                    ".mdsl",
                    ".mechanics"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "mechanicsdsl",
                "scopeName": "source.mechanicsdsl",
                "path": "./syntaxes/mechanicsdsl.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "mechanicsdsl",
                "path": "./snippets/mechanicsdsl.json"
            }
        ]
    }
}